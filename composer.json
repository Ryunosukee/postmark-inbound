{
    "name": "mvdnbrk/postmark-inbound",
    "type": "library",
    "description": "API to process Postmark Inbound Webhooks.",
    "keywords": [
        "postmark",
        "e-mail",
        "inbound",
        "webhook"
    ],
    "homepage": "https://github.com/mvdnbrk/postmark-inbound",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "homepage": "https://github.com/mvdnbrk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "tightenco/collect": "^5.8 || ^6.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5 || ^8.0",
        "scrutinizer/ocular": "^1.6",
        "symfony/var-dumper": "^4.3 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Postmark\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Postmark\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
