{
    "name": "dcorreah/postmark-inbound",
    "type": "library",
    "description": "API to process Postmark Inbound Webhooks.",
    "keywords": [
        "postmark",
        "e-mail",
        "inbound",
        "webhook"
    ],
    "homepage": "https://github.com/mvdnbrk/postmark-inbound",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hincapie",
            "email": "daniel.hincapie972423@gmail.com",
            "homepage": "https://github.com/ryunosukee",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.4 || ^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "dcorreah\\Postmark\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dcorreah\\Postmark\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
